+	case SC_XOPEN_ENH_I18N:
+		return _XOPEN_ENH_I18N, nil
+	case SC_XOPEN_REALTIME:
+		return _XOPEN_REALTIME, nil
+	case SC_XOPEN_REALTIME_THREADS:
