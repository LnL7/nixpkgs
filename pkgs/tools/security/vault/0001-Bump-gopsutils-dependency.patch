+
+func getNprocs() int64 {
+	count, err := getNprocsSysfs()
+	if err == nil {
+		return count
+	}
+
+	count, err = getNprocsProcStat()
+	if err == nil {
+		return count
+	}
+
+	// default to the value determined at runtime startup if all else fails
+	return int64(runtime.NumCPU())
